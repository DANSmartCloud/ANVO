		</app>
		<app name="startupopt" chname="启动项管理" ipc_node="2,1,80" ipc_id="1140002" nocheck="">
			<option id="1" ico_classic="pcspeedup_1.svg" ico_pioneer="pcspeedup_2.svg" title="检测到可禁用的开机启动项" subtitle1="建议禁用，提升开机速度" subtitle2="" btn_classic="立即禁用" btn_pioneer="禁用" btn_cfg="V15_Mini_Blue_Button" cmdtype="" cmdparam="">
				<!--1代表小界面，2代表大界面，只有上面的cmdtype和cmdparam都为空时才解析该子节点-->
				<cmd uitype="1" cmdtype="1" cmdparam="/pcmgr /TAB_SYSSTARTUP /QuickScan"/>
				<cmd uitype="2" cmdtype="1" cmdparam="/pcmgr /TAB_SYSSTARTUP /ShowBootMgrDlg"/>
			</option>
		</app>
		<app name="softmgr" chname="软件管理" ipc_node="" ipc_id="" nocheck="1">
