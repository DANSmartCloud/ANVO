			<option id="2" ico_classic="garbageclean_1.svg" ico_pioneer="garbageclean_2.svg" title="想让电脑更快？" subtitle1="垃圾清理帮你一键搞定" subtitle2="" btn_classic="一键清理" btn_pioneer="清理" btn_cfg="V15_Mini_Blue_Button" cmdtype="1" cmdparam="/TAB_SYSCLEAN /SysScan"/>
